{
  "apiVersion": "v1",
  "kind": "Pod",
  "metadata": {
    "namespace": "new-namespace",
    "name": "nginx"
  },
  "spec": {
    "containers": [
      {
        "name": "nginx",
        "image": "nginx",
        "ports": [
          {
            "containerPort": 80
          }
        ]
      },
      {
        "name": "apac",
        "image": "apache",
        "ports": [
          {
            "containerPort": 90
          }
        ]
      }
    ]
  }
}